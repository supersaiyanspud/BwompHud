"UnlitGeneric"
{
   "$basetexture" "console/background_2fort_widescreen"
   "$vertexcolor" 		1 
   "$vertexalpha" 		1 
   "$nolod" 		1
   "$translucent"		1
   "Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "5"
      }
   }
}
